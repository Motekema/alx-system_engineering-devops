#!/usr/bin/env bash
# This script stops the 4-to_infinity_and_beyond process using kill.

# Find the process ID (PID) of the 4-to_infinity_and_beyond script.
pid=$(pgrep -f "4-to_infinity_and_beyond")

if [ -n "$pid" ]; then
    # If the process is running, send a termination signal to it.
    kill "$pid"
    echo "Terminated"
else
    echo "Process not found"
fi

